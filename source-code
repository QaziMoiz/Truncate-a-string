function truncateString(str, num) {
  if(num <= 3){
    var a = str.slice(0,num);
    a = a.concat("...");
    return(a);
  }else if (str.length > num){
    var result = str.slice(0,num - 3);
    result = result.concat("...");
    return (result);
  }else if(str.length <= num){
    return (str);
}
}
truncateString("A-", 1);
